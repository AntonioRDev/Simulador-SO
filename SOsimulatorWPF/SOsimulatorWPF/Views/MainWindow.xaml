<Window x:Class="SOsimulatorWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SOsimulatorWPF"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen" Background="Sienna"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Button Grid.Column="1" VerticalAlignment="Bottom" Margin="0,0,0,10" Height="40" Content="Preemptivo" Click="Preemptivo_Click"/>
        <Button Grid.Column="1" Grid.Row="1" VerticalAlignment="Top" Margin="0,10,0,0" Height="40" Content="Não Preemptivo" Click="NaoPreemptivo_Click"/>
    </Grid>
</Window>
